{
  "name": "evergreen-github-companion",
  "version": "1.0.0",
  "private": "true",
  "description": "Helps you keep your GitHub profile green (and not through illegitimate means).",
  "main": "out/electron/index.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "watch": "npm-run-all --parallel build:watch start:watch",
    "clean": "rm -rf out && npm run build",
    "start": "cd ./out/electron && electron ./index.js",
    "start:watch": "nodemon --watch out --exec npm start",
    "test": "echo \"No tests. specified\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --config .prettierrc.js * --write",
    "prepare": "husky install",
    "precommit": "npm run lint:fix && npm run format && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewkolos/evergreen-github-companion.git"
  },
  "author": "andrew-kolos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewkolos/evergreen-github-companion/issues"
  },
  "homepage": "https://github.com/andrewkolos/evergreen-github-companion#readme",
  "dependencies": {
    "@akolos/event-emitter": "^3.0.0",
    "@octokit/rest": "^18.12.0",
    "cron-time-generator": "^1.3.0",
    "dotenv": "^16.0.1",
    "electron": "^18.2.4",
    "electron-store": "^8.0.1",
    "fs-extra": "^10.1.0",
    "node-cron": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "simple-git": "^3.7.1",
    "ts-key-enum": "^2.0.11"
  },
  "devDependencies": {
    "@akolos/eslint-config-typescript": "^2.0.0",
    "@akolos/prettier-config": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/node-cron": "^3.0.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.6.0",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-shell-plugin-next": "^2.2.2"
